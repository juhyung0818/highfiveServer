<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.highfive.mapper.FavoritesMapper">

	<insert id="regist">
		insert into tbl_favorites (uid, bno )
		values (#{uid},#{bno})
	</insert>
	
	<delete id="delete">
 		delete from tbl_favorites where fno=#{fno}
 	</delete>
 	

 	<select id="listAll" resultType="org.highfive.domain.FavoritesVO">
 	 	select  fno, uid, bno
 		from tbl_favorites where fno > 0 
 		order by fno desc
 	</select>
</mapper>